\newcommand{\dphidr}{\derivitivetwo{\phi}{r}}
\newcommand{\dphidt}{\derivitivetwo{\phi}{t}}
\newcommand{\dphidx}{\derivitivetwo{\phi}{x}}
\newcommand{\dphidy}{\derivitivetwo{\phi}{y}}
\newcommand{\ppphi}{\partialone{\phi}}
\newcommand{\pphiptheta}{\partialtwo{\phi}{\theta}}
\newcommand{\pphipr}{\partialtwo{\phi}{r}}
\newcommand{\pphipt}{\partialtwo{\phi}{t}}
\newcommand{\pphiptn}[1]{\partialtwon{\phi}{t}{#1}}


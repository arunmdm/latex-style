%
% The many shortcuts for derivitives, seperated from -style-math because there
% are just so many of these
%
\NeedsTeXFormat{LaTeX2e}[1995/12/01]
\ProvidesClass{school-style-derivitives}

\newcommand{\derivitive}{\fsp\mathrm{d}}
\newcommand{\derivitiveone}[1]{\frac{\mathrm{d}}{\mathrm{d}#1}}
\newcommand{\derivitiveonen}[2]{\frac{\mathrm{d}^{#2}}{\mathrm{d}#1^{#2}}}
\newcommand{\derivitivetwo}[2]{\frac{\mathrm{d}#1}{\mathrm{d}#2}}
\newcommand{\derivitivetwon}[3]{\frac{\mathrm{d}^{#3}#1}{\mathrm{d}#2^{#3}}}
\newcommand{\partialone}[1]{\frac{\partial}{\partial #1}}
\newcommand{\partialonen}[2]{\frac{\partial^{#2}}{\partial #1^{#2}}}
\newcommand{\partialtwo}[2]{\frac{\partial #1}{\partial #2}}
\newcommand{\partialtwon}[3]{\frac{\partial^{#3} #1}{\partial #2 ^{#3}}}

\newcommand{\dll}{\derivitive \ell}
\newcommand{\dvecell}{\derivitive \vec{\ell}}

\newcommand{\drho}{\derivitive \rho}
\newcommand{\ddrho}{\derivitiveone{\rho}}
\newcommand{\prhofpt}{\partialtwo{\rho_f}{t}}
\newcommand{\prhopt}{\partialtwo{\rho}{t}}

\newcommand{\dPhidt}{\derivitivetwo{\Phi}{t}}

\newcommand{\dphi}{\derivitive \phi}
